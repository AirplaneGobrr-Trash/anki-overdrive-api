    "nyc": "^11.4.1",
    "sinon": "^4.2.1",
    "source-map-support": "^0.5.0",
    "testdouble": "^3.3.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "typings": "^1.3.1"
  },
  "nyc": {
    "exclude": [
      "node_modules/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/noble": "0.0.34",
